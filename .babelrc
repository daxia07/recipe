{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 5 versions",
            "ie >= 8"
          ]
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-runtime","@babel/plugin-syntax-dynamic-import"],
  "sourceMaps": "both",
  "retainLines": true
}